# OpenCorpInsight DART MCP Server 환경 변수 설정

# DART Open API 키 (필수)
# https://opendart.fss.or.kr 에서 발급받은 40자리 API 키
DART_API_KEY=your_40_character_api_key_here

# 캐시 설정
DART_CACHE_DIR=./cache
CACHE_TTL_HOURS=24

# 로깅 설정
LOG_LEVEL=INFO
LOG_DIR=./logs

# MCP 서버 설정
MCP_SERVER_NAME=dart-mcp-server
MCP_SERVER_VERSION=0.1.0

# 개발 모드 (선택사항)
DEVELOPMENT_MODE=false

# API 호출 제한 (선택사항)
API_RATE_LIMIT=100  # 분당 최대 호출 수
API_TIMEOUT=30      # API 타임아웃 (초) 